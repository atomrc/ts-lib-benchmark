{
  "name": "ts-compilation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "all": "rm -fr dist && pnpm esbuild && pnpm tsup && pnpm swc && pnpm tsc && pnpm prettier",
    "esbuild": "esbuild src/*.ts --target=es2017 --tree-shaking=true --outdir=./dist/esbuild",
    "tsup": "tsup src/*.ts --format esm --target=es2017 --treeshake -d=./dist/tsup",
    "swc": "swc -d ./dist/swc -C jsc.target=es2017 src/*.ts",
    "tsc": "tsc --outDir ./dist/tsc",
    "prettier": "prettier -w dist/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/lodash": "^4.14.191",
    "esbuild": "^0.17.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.3",
    "swc": "^1.0.11",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.27"
  }
}
